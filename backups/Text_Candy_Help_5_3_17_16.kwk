<project id="Text_Candy_Help">  <settings>    <name>Text_Candy_Help</name>    <media>Book</media>    <device>iPad</device>    <OS>iOS</OS>    <zoom>zoomStretch</zoom>    <width>1024</width>    <height>768</height>    <orientation>Landscape</orientation>    <resolution>132</resolution>    <bookmark>false</bookmark>    <swipe>true</swipe>    <pinch>false</pinch>    <thumbs>false</thumbs>    <libraries>      <lib id="TextCandy">        <name>TextCandy</name>        <path>~/Documents/Kwik/Text_Candy_Help/lib_text_candy.lua</path>      </lib>    </libraries>  </settings>  <pages>    <page id="page1">      <name>page1</name>      <number>1</number>      <file>/Users/Hector/Documents/Kwik/Text_Candy_Help/page1.psd</file>      <fileName>page1.psd</fileName>      <replacement id="bg">        <name>bg</name>        <type>Vector</type>        <layer>bg</layer>        <vector>Rectangle</vector>        <width>1024</width>        <height>768</height>        <color>10, 213, 251</color>      </replacement>      <replacement id="Header">        <name>Header</name>        <type>Vector</type>        <layer>Header</layer>        <vector>Rectangle</vector>        <width>502</width>        <height>60</height>        <color>245, 4, 33</color>      </replacement>      <replacement id="Counter">        <name>Counter</name>        <type>Vector</type>        <layer>Counter</layer>        <vector>Rectangle</vector>        <width>387</width>        <height>67</height>        <color>4, 21, 245</color>      </replacement>      <replacement id="countDown">        <name>countDown</name>        <type>Vector</type>        <layer>countDown</layer>        <vector>Rectangle</vector>        <width>175</width>        <height>68</height>        <color>12, 245, 143</color>      </replacement>      <code id="code_TextCandyHelper">        <name>code_TextCandyHelper</name>        <code/>        <position>middle</position>        <extFile>~/Documents/Kwik/Text_Candy_Help/TextCandyHelper.lua</extFile>      </code>      <hide id="countDown">        <layer>countDown</layer>        <name>countDown</name>        <target>Layer</target>      </hide>      <hide id="Counter">        <layer>Counter</layer>        <name>Counter</name>        <target>Layer</target>      </hide>      <hide id="Header">        <layer>Header</layer>        <name>Header</name>        <target>Layer</target>      </hide>      <variables>        <variable id="countNumber">          <name>countNumber</name>          <value>0</value>          <comment>--Counter</comment>          <table>false</table>          <save>false</save>          <type>local</type>          <content>Number</content>          <where>before</where>        </variable>        <variable id="headerName">                    <name>headerName</name>          <value></value>          <comment>TextCandy Header Text</comment>          <table>false</table>          <save>false</save>          <type>local</type>          <content>Formula</content>          <where>before</where>        </variable>        <variable id="counterName">                    <name>counterName</name>          <value></value>          <comment>Text Candy Counter Text</comment>          <table>false</table>          <save>false</save>          <type>local</type>          <content>Formula</content>          <where>before</where>        </variable>      </variables>      <trigger id="act_countUp">        <name>act_countUp</name>        <action id="var_063">          <name>var_063</name>          <type>Edit Variable</type>          <variable>countNumber</variable>          <value>countNumber + 1</value>        </action>      </trigger>      <button id="but_countUP">        <name>but_countUP</name>        <layer>counterButton</layer>        <rollover>counterButton</rollover>        <target>Layer</target>        <type>Tap</type>        <tap>1</tap>        <action id="act_728">          <name>act_728</name>          <type>Play Action</type>          <trigger>act_countUp</trigger>        </action>        <action id="code_862">          <name>code_862</name>          <type>External Code</type>          <code/>          <extFile>~/Documents/Kwik/Text_Candy_Help/countUp.lua</extFile>        </action>      </button>    </page>  </pages></project>